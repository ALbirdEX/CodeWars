if (flower1%2==0&&flower2%2!==0) {
        return true;
    }else if(flower1%2!==0&&flower2%2==0){
        return true;
    }else return false;


function pillars(num_pill, dist, width) {
  return num_pill == 1  ? 0 : ((num_pill-2)*width) + ((100*dist)*(num_pill-1))
  
  function filter_list(l) {
  return l.filter((e) => {
    return typeof e !== 'string'
  })
}
